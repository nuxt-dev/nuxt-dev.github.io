{
  "name": "official-website",
  "version": "1.0.0",
  "description": "Official website for Nuxt.js",
  "repository": {
    "type": "git",
    "url": "https://gitea.rxreader.com/jsf-new/official-website.git"
  },
  "keywords": [
    "nuxt",
    "nuxt.js",
    "vue",
    "vue.js",
    "vuejs"
  ],
  "author": {
    "name": "lin713"
  },
  "private": true,
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prelint": "nuxt prepare",
    "lint": "eslint \"{app,modules,server}/**/*.{vue,ts}\"",
    "prelint-fix": "nuxt prepare",
    "lint-fix": "eslint \"{app,modules,server}/**/*.{vue,ts}\" --fix",
    "typecheck": "nuxt typecheck",
    "format": "prettier --write \"{app,modules,server}/**/*.{ts,vue}\"",
    "license-checker": "license-checker --excludePrivatePackages --production --onlyAllow=\"ISC;BSD-3-Clause;BSD-2-Clause;MIT;Apache-2.0;0BSD;Python-2.0\""
  },
  "dependencies": {
    "@nuxt/eslint": "^0.3.13",
    "@nuxt/image": "^1.7.0",
    "nuxt": "^3.12.2",
    "vue": "^3.4.29",
    "vue-router": "^4.3.3"
  },
  "devDependencies": {
    "license-checker": "^25.0.1",
    "prettier": "^3.3.2",
    "typescript": "5.4.4",
    "vue-tsc": "^1.8.27"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
